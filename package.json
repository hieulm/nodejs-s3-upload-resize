{
  "name": "photo-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/jest --config=jest.config.js",
    "test:watch": "./node_modules/.bin/jest --watch --config=jest.config.js",
    "test:coverage": "./node_modules/.bin/jest --coverage --collectCoverageFrom=src/**/*.js",
    "precommit": "lint-staged",
    "start": "node src/index.js",
    "dev": "./node_modules/.bin/nodemon --inspect=0.0.0.0:9229 src/index.js",
    "debug": "node --inspect-brk=0.0.0.0:9229 src/index.js",
    "lint": "./node_modules/.bin/eslint src",
    "lintfix": "./node_modules/.bin/eslint src --fix",
    "format": "./node_modules/.bin/prettier --write './src/**/*.js'"
  },
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{js,jsx}": [
        "eslint --no-ignore --fix",
        "git add --force"
      ],
      "*.{json,md,graphql}": [
        "prettier --write",
        "git add --force"
      ]
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "aws-sdk": "^2.636.0",
    "body-parser": "^1.19.0",
    "busboy": "^0.3.1",
    "compression": "^1.7.4",
    "config": "^3.3.0",
    "connect-rid": "0.0.1",
    "express": "^4.17.1",
    "express-validation": "^3.0.2",
    "helmet": "^3.21.3",
    "http-status": "^1.4.2",
    "morgan": "^1.9.1",
    "response-time": "^2.3.2",
    "sharp": "^0.25.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "decache": "^4.5.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^23.5.0",
    "lint-staged": "^8.1.0",
    "mocha": "^5.1.1",
    "nodemon": "^1.18.7",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.2",
    "rewire": "^4.0.1",
    "sinon": "^8.0.4",
    "supertest": "^3.4.2"
  }
}
